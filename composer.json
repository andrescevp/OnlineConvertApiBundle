{
  "name": "andrescevp/OnlineConvert",
  "description": "Integrates php-amqplib with Symfony2 and RabbitMq",
  "keywords": ["symfony2"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [{
    "name" : "Alvaro Videla"
  }],
  "require": {
    "php"                          : ">=5.3.0",
    "symfony/dependency-injection" : "~2.3",
    "symfony/event-dispatcher"     : "~2.3",
    "symfony/config"               : "~2.3",
    "symfony/yaml"                 : "~2.3",
    "symfony/console"              : "~2.3",
    "videlalvaro/php-amqplib"      : "~2.6"
  },
  "suggest": {
    "symfony/framework-bundle" : "To use this lib as a full Symfony Bundle and to use the profiler data collector"
  },
  "require-dev": {
    "symfony/serializer": "~2.3",
    "symfony/debug"     : "~2.3",
    "phpunit/phpunit" : ">=3.7.0"
  },
  "autoload": {
    "psr-4": {
      "Aacp\\OnloneConvertBundle\\": "OnlineConvertApiBundle"
    }
  }
}